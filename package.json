{
  "name": "hawar-website",
  "private": true,
  "license": "MIT",
  "version": "0.0.0",
  "homepage": "https://ascoor.github.io/hawar.club/",
  "type": "module",
  "scripts": {
    "build-gh-pages": "vite build --mode gh-pages",
    "deploy": "yarn build-gh-pages && gh-pages -d dist",
    "ins-new": "rm -rf node_modules dist yarn.lock && yarn install && yarn build",
    "dev": "vite --host 0.0.0.0",
    "format": "prettier --write .",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean-deploy": "find /home/hawar.club/public_html -mindepth 1 \\( -not -name 'hawar.club' -not -path '/home/hawar.club/public_html/*'  \\) -exec rm -rf {} + && cp -r dist/* /home/hawar.club/public_html"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.20",
    "framer-motion": "^11.16.0",
    "postcss": "^8.4.49",
    "react": "^19.0.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.4.0",
    "react-lazy-load-image-component": "^1.6.3",
    "react-router-dom": "^7.1.1",
    "react-scroll": "^1.9.0",
    "swiper": "^11.2.0",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "prettier": "^3.4.2",
    "terser": "^5.37.0",
    "vite": "^6.0.5",
    "vite-plugin-sitemap": "^0.7.1"
  }
}
