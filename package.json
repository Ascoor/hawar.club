{
  "name": "hawar-website",
  "private": true,
  "license": "MIT",
  "version": "0.0.0", 
  "homepage": "https://ascoor.github.io/hawar.club",
  "type": "module",
"scripts": {
  "predeploy": "yarn build",
  "deploy": "gh-pages -d dist",
    "ins-new": "rm -rf node_modules dist yarn.lock && yarn install && yarn build",
    "dev": "vite --host 0.0.0.0",
    "format": "prettier --write .",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "clean-deploy": "find /home/hawar.club/public_html -mindepth 1 \\( -not -name 'hawar.club' -not -path '/home/hawar.club/public_html/*'  \\) -exec rm -rf {} + && cp -r dist/* /home/hawar.club/public_html"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "^1.3.1",
    "@heroicons/react": "^2.2.0",
    "animate.css": "^4.1.1",
    "aos": "^2.3.4",
    "autoprefixer": "^10.4.20",
    "framer-motion": "^11.13.5",
    "motion": "^11.13.5",
    "postcss": "^8.4.49",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-intersection-observer": "^9.13.1",
    "react-scroll": "^1.9.0",
    "swiper": "^11.1.15",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.12.0",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.4.2",
    "terser": "^5.37.0",
    "vite": "^6.0.5"
  }
}
